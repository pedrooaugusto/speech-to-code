digraph GoToLine {
    id="GoToLine";
    label="Trocar linha";
    lang="pt-BR";
    langName="Português";
    pad="0.2";
    rankdir=LR;
    node[shape=doublecircle]; 4;
    node[shape=circle];

    0 -> 1 [label="(vá, vai)"];
    1 -> 2 [label="(para)"];
    2 -> 5 [label="(a)"];
    {2 1 0 5} -> 3 [label="(linha)"];
    3 -> 4 [label="({numeral})", store=line];

    title="Trocar linha";
    desc="Move or cursor para uma linha diferente";
}